{
  "schemaVersion": 1,
  "id": "meteor-auto-meow",
  "version": "${version}",
  "name": "AutoMeow",
  "description": "AutoMeow ported from faghack to meteor addon",
  "authors": [
    "ItzMarni"
  ],
  "icon": "assets/automeow/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.marni.addon.AutoMeow"
    ]
  },
  "mixins": [
    "automeow.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
